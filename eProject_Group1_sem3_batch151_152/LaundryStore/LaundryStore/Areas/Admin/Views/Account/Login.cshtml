<!DOCTYPE html>
<html lang="en">
<head>
    <title>Login V15</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--===============================================================================================-->
    <link rel="icon" type="image/png" href="~/Assets/Admin/vendors/loginPage/images/icons/favicon.ico" />
    <!--===============================================================================================-->
    <link href="~/Assets/Admin/vendors/loginPage/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <!--===============================================================================================-->
    <link href="~/Assets/Admin/vendors/loginPage/fonts/font-awesome-4.7.0/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
    <!--===============================================================================================-->
    <link href="~/Assets/Admin/vendors/loginPage/fonts/Linearicons-Free-v1.0.0/icon-font.min.css" rel="stylesheet" type="text/css" />
    <!--===============================================================================================-->
    <link href="~/Assets/Admin/vendors/loginPage/vendor/animate/animate.css" rel="stylesheet" type="text/css" />
    <!--===============================================================================================-->
    <link href="~/Assets/Admin/vendors/loginPage/vendor/css-hamburgers/hamburgers.min.css" rel="stylesheet" type="text/css" />
    <!--===============================================================================================-->
    <link href="~/Assets/Admin/vendors/loginPage/vendor/animsition/css/animsition.min.css" rel="stylesheet" type="text/css" />
    <!--===============================================================================================-->
    <link href="~/Assets/Admin/vendors/loginPage/vendor/select2/select2.min.css" rel="stylesheet" type="text/css" />
    <!--===============================================================================================-->
    <link href="~/Assets/Admin/vendors/loginPage/vendor/daterangepicker/daterangepicker.css" rel="stylesheet" type="text/css" />
    <!--===============================================================================================-->
    <link href="~/Assets/Admin/vendors/loginPage/css/util.css" rel="stylesheet" type="text/css" />
    <!--===============================================================================================-->
    <link href="~/Assets/Admin/vendors/loginPage/css/main.css" rel="stylesheet" type="text/css" />
    <!--===============================================================================================-->
</head>
<body>

    <div class="limiter">
        <div class="container-login100">
            <div class="wrap-login100">
                <div class="login100-form-title" style="background-image: url('/Assets/Admin/vendors/loginPage/images/bg-01.jpg');">
                    <span class="login100-form-title-1">
                        Sign In
                    </span>
                </div>

                <form class="login100-form validate-form">
                    <div class="wrap-input100 validate-input m-b-26" data-validate="Username is required">
                        <span class="label-input100">Username</span>
                        <input class="input100" type="text" name="email" placeholder="Enter email">
                        <span class="focus-input100"></span>
                    </div>

                    <div class="wrap-input100 validate-input m-b-18" data-validate="Password is required">
                        <span class="label-input100">Password</span>
                        <input class="input100" type="password" name="pass" placeholder="Enter password">
                        <span class="focus-input100"></span>
                    </div>

                    <div class="flex-sb-m w-full p-b-30">
                        <div class="contact100-form-checkbox">
                            <input class="input-checkbox100" id="ckb1" type="checkbox" name="remember-me">
                            <label class="label-checkbox100" for="ckb1">
                                Remember me
                            </label>
                        </div>

                        <div>
                            <a href="#" class="txt1">
                                Forgot Password?
                            </a>
                        </div>
                    </div>

                    <div class="container-login100-form-btn" style="padding-left:25%">
                        <button class="login100-form-btn">
                            Login
                        </button>
                    </div>
                </form>
            </div>
        </div>
    </div>

    <!--===============================================================================================-->
    <script src="~/Assets/Admin/vendors/loginPage/vendor/jquery/jquery-3.2.1.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/Assets/Admin/vendors/loginPage/vendor/animsition/js/animsition.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/Assets/Admin/vendors/loginPage/vendor/bootstrap/js/popper.js"></script>
    <!--===============================================================================================-->
    <script src="~/Assets/Admin/vendors/loginPage/vendor/bootstrap/js/bootstrap.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/Assets/Admin/vendors/loginPage/vendor/select2/select2.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/Assets/Admin/vendors/loginPage/vendor/daterangepicker/moment.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/Assets/Admin/vendors/loginPage/vendor/daterangepicker/daterangepicker.js"></script>
    <!--===============================================================================================-->
    <script src="~/Assets/Admin/vendors/loginPage/vendor/countdowntime/countdowntime.js"></script>
    <!--===============================================================================================-->
    <script src="~/Assets/Admin/vendors/loginPage/js/main.js"></script>
</body>
</html>