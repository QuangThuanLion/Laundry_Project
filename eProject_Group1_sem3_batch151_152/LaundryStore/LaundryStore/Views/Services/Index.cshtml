<!-- quote start -->
<section class="quote-service-se">
    <div class="container">
        <div class="row no-gutters">
            <div class="col-lg-7">
                <div class="bg-quotes bg-cover h-100" style=" background-image: url('/Assets/Client/vendors/img/bg/quote.jpg');">
                </div>
            </div>
            <div class="col-lg-5 text-center">
                <div class="quote-service h-100" data-overlay="6" style="background-image: url('img/bg/quote-bg.jpg');">
                    <h3 class="z-5 fs-28 f-700 white mb-20">Get a Free Quote</h3>
                    <form action="#" class="z-5">
                        <div class="row">
                            <div class="form-group col-lg-12 col-md-4 col-sm-6 relative">
                                <input type="text" class="form-control input-white shadow-2" id="zipCode" placeholder="Your Zip Code">
                                <i class="fas fa-map-marker-alt transform-v-center"></i>
                            </div>
                            <div class="form-group col-lg-12 col-md-4 col-sm-6 relative">
                                <input type="text" class="form-control input-white shadow-2" id="name" placeholder="Your Name">
                                <i class="far fa-user transform-v-center"></i>
                            </div>
                            <div class="form-group col-lg-12 col-md-4 col-sm-6 relative">
                                <input type="email" class="form-control input-white shadow-2" id="Email" placeholder="Your Email">
                                <i class="far fa-envelope transform-v-center"></i>
                            </div>
                            <div class="form-group col-lg-12 col-md-4 col-sm-6 relative">
                                <input type="text" class="form-control input-white shadow-2" id="Phone" placeholder="Phone Number">
                                <i class="fas fa-phone transform-v-center"></i>
                            </div>
                            <div class="form-group sp-padd-sel1 col-lg-6 col-md-4 col-sm-6 col-sm-6 relative">
                                <select class="form-control input-white select-nice" id="Bedroom">
                                    <option>Bedroom</option>
                                    <option>1</option>
                                    <option>2</option>
                                    <option>3</option>
                                    <option>4</option>
                                    <option>5</option>
                                </select>
                            </div>
                            <div class="form-group sp-padd-sel2 col-lg-6 col-md-4 col-sm-6 relative">
                                <select class="form-control input-white select-nice" id="Bathroom">
                                    <option>Bathroom</option>
                                    <option>1</option>
                                    <option>2</option>
                                    <option>3</option>
                                    <option>4</option>
                                    <option>5</option>
                                </select>
                            </div>
                            <div class="form-group col-lg-12 mb-0">
                                <button type="submit" class="btn btn-black shine-btn btn-block">GET FREE QUOTE</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</section>
